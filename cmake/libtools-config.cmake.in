@PACKAGE_INIT@

INCLUDE("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake")

SET(@PROJECT_NAME@_FOUND TRUE)
MARK_AS_ADVANCED(@PROJECT_NAME@_FOUND)

SET(@PROJECT_NAME@_mysql_FOUND @MYSQL_FOUND@ CACHE BOOL "@PROJECT_NAME@ with MySQL")
MARK_AS_ADVANCED(@PROJECT_NAME@_mysql_FOUND)

SET(@PROJECT_NAME@_uriparser_FOUND @URIPARSER_FOUND@ CACHE BOOL "@PROJECT_NAME@ with uriparser")
MARK_AS_ADVANCED(@PROJECT_NAME@_uriparser_FOUND)

CHECK_REQUIRED_COMPONENTS(@PROJECT_NAME@)

IF(@PROJECT_NAME@_FOUND)
	MESSAGE(STATUS "Found @PROJECT_NAME@: ${CMAKE_CURRENT_LIST_DIR} (found version \"${@PROJECT_NAME@_VERSION}\")")

	IF(@PROJECT_NAME@_FIND_COMPONENTS)
		MESSAGE(STATUS "Found the following @PROJECT_NAME@ components:")

		FOREACH(comp ${@PROJECT_NAME@_FIND_COMPONENTS})
			MESSAGE(STATUS "   ${comp}")
		ENDFOREACH()
	ENDIF()
ENDIF()
